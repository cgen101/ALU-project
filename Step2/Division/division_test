T_0.0 ;
    %load/vec4 v0x561b2a3a77b0_0;
    %store/vec4 v0x561b2a3fa5c0_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561b2a3fa6a0_0, 0, 4;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x561b2a3fa2c0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x561b2a3fa2c0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x561b2a3fa6a0_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x561b2a3fa5c0_0;
    %parti/s 1, 3, 3;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x561b2a3fa6a0_0, 0, 4;
    %load/vec4 v0x561b2a3fa5c0_0;
    %parti/s 3, 0, 2;
    %concati/vec4 0, 0, 1;
    %store/vec4 v0x561b2a3fa5c0_0, 0, 4;
    %load/vec4 v0x561b2a3fa2c0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x561b2a3fa2c0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
