    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561b2a3fa780_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x561b2a3fa3b0_0, 0, 4;
    %load/vec4 v0x561b2a3a77b0_0;
    %store/vec4 v0x561b2a3fa490_0, 0, 4;
