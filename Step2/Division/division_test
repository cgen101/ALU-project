    %store/vec4 v0x561b2a3fa490_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561b2a3fa780_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x561b2a3e3290;
T_1 ;
